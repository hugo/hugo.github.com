{
  "compilerOptions": {
    "noEmit": true,
    "esModuleInterop": true,
    "module": "es2020",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "target": "es2020",
    "strict": true,
    "lib": ["dom", "es2020"],
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true
  }
}
